{
  "manifest_version": 3,
  "name": "test http request",
  "version": "1.0",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "host_permissions": [
    "http://127.0.0.1/",
    "http://www.example.com/",
    "https://game.maj-soul.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "http://127.0.0.1/",
        "http://www.example.com/",
        "https://game.maj-soul.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}